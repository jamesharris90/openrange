# Copy this file to .env and fill in your Saxo credentials
SAXO_TOKEN=your_saxo_token_here
SAXO_CLIENT_KEY=your_client_key_here
SAXO_ACCOUNT_NUMBER=your_account_number_here
SAXO_API_URL=https://gateway.saxobank.com/openapi
PORT=3000
REQUEST_TIMEOUT=10000
NODE_ENV=development
# Proxy protection: set a random value and keep it secret
PROXY_API_KEY=replace_with_secure_random_string
